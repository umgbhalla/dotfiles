lapsed":false,"created_ts":1591452227701,"edited_ts":1591452214063,"attached_ts":1591452214063,"sync_attempt":0,"children_loc_ids":[],"children_by_loc_ids":{}},{"id":"CzJixaK3MDoHKMD8Ck74w_w4","meta":"{\"c\":\"Design from the Corners file: this is the exact file from the demo. Play around with turning layers on and off and dragging the artwork to the swatches panel to see how tiles are built. This opens in Illustrator.\"}","collapsed":false,"created_ts":1591452227700,"edited_ts":1591452214063,"attached_ts":1591452214063,"sync_attempt":0,"children_loc_ids":[],"children_by_loc_ids":{}},{"id":"DidopZIvFO7h27m7KE6NQaqX","meta":"{\"c\":\"[Color game](http://color.method.ac/): from method of action. So edutaining! \"}","collapsed":false,"created_ts":1591452227706,"edited_ts":1591452214063,"attached_ts":1591452214063,"sync_attempt":0,"children_loc_ids":[],"children_by_loc_ids":{}},{"id":"GMZ0CX5PWeTVmMWHsapgO5bc","meta":"{\"c\":\"Pick three photos you're repelled by and three you're attracted to, and comlete the \\\"repel and attract\\\" pattern funtivity to start being able to articulate your personal brand of color. \"}","collapsed":false,"created_ts":1591452227669,"edited_ts":1591452214063,"attached_ts":1591452214063,"sync_attempt":0,"children_loc_ids":[],"children_by_loc_ids":{}},{"id":"e-26YkDA4f9MXX87hGrShrfA","meta":"{\"c\":\"Still/Moving\"}","collapsed":false,"created_ts":1591452227695,"edited_ts":1591452214063,"attached_ts":1591452214063,"sync_attempt":0,"children_loc_ids":[],"children_by_loc_ids":{}},{"id":"TW7hBaiYKxK4M6vLC_tGXME6","meta":"{\"c\":\"Thanks so much for participating in Intro to Surface Design! Stay tuned for the next installment :D\"}","collapsed":false,"created_ts":1591452227687,"edited_ts":1591452214063,"attached_ts":1591452214063,"sync_attempt":0,"children_loc_ids":[],"children_by_loc_ids":{}},{"id":"8Iq4NxCpGn8EqtFKTZo0EyRO","meta":"{\"c\":\"Smooth/Rough\"}","collapsed":false,"created_ts":1591452227692,"edited_ts":1591452214063,"attached_ts":1591452214063,"sync_attempt":0,"children_loc_ids":[],"children_by_loc_ids":{}},{"id":"RQcaOlBwX6xD6eg3gFG0Wywq","meta":"{\"c\":\"Patterns Practice: This is a multi-artboard pdf that you can open in illustrator and play around with to practice making directional and non-directional patterns. Directions Included.\"}","collapsed":false,"created_ts":1591452227700,"edited_ts":1591452214063,"attached_ts":1591452214063,"sync_attempt":0,"children_loc_ids":[],"children_by_loc_ids":{}},{"id":"hPfPCy6OrYADmNKKsfnE0mL_","meta":"{\"c\":\"Tone it downAfter you've settled on a formal idea for your third tile, it's time to address the color situation. \"}","collapsed":false,"created_ts":1591452227683,"edited_ts":1591452214063,"attached_ts":1591452214063,"sync_attempt":0,"children_loc_ids":[],"children_by_loc_ids":{}},{"id":"qy3R49AvMl_MzZ1aGmSgCN0r","meta":"{\"c\":\"Liquid/Solid\"}","collapsed":false,"created_ts":1591452227697,"edited_ts":1591452214063,"attached_ts":1591452214063,"sync_attempt":0,"children_loc_ids":[],"children_by_loc_ids":{}},{"id":"5rFDvtp2rh8oWKW1Cv0VL6JC","meta":"{\"c\":\"Large/Small\"}","collapsed":false,"created_ts":1591452227691,"edited_ts":1591452214063,"attached_ts":1591452214063,"sync_attempt":0,"children_loc_ids":[],"children_by_loc_ids":{}},{"id":"jZpnL7Gsth981ocO7RXD55tI","meta":"{\"c\":\"For example, if your contrasting tiles are horizontal and vertical, try to balance with something diagonal. If your tiles are flowers and stripes, try balancing with polka dots. \"}","collapsed":false,"created_ts":1591452227683,"edited_ts":1591452214063,"attached_ts":1591452214063,"sync_attempt":0,"children_loc_ids":["lTllxozCEVOh_ov7Ti07ANZ9"],"children_by_loc_ids":{"lTllxozCEVOh_ov7Ti07ANZ9":"hPfPCy6OrYADmNKKsfnE0mL_"}},{"id":"UTHGPP-NiUVv1dzObDZLvdG7","meta":"{\"c\":\"![cae257a9](http://s3.amazonaws.com/skillshare/uploads/session/tmp/cae257a9)\"}","collapsed":false,"created_ts":1591452227667,"edited_ts":1591452214063,"attached_ts":1591452214063,"sync_attempt":0,"chi